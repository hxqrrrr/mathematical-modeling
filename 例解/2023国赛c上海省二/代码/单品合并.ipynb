   "source": [
    "import pandas as pd\n",
    "\n",
    "df = pd.read_excel('单品销售量.xlsx')\n",
    "\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "metadata": {},
   "outputs": [],
   "source": [
    "import re\n",
    "\n",
    "df['单品名称'] = df['单品名称'].apply(lambda x: re.sub(r'\\(\\d+\\)', '', x))\n",
    "df_merged = df.groupby('单品名称')['单品销量和'].sum().reset_index()\n",
    "df_merged.to_excel('单品销售量（合并单品）.xlsx', index=False)\n"
   ]
  }
